    pod 'paper-onboarding'
    pod 'CircleMenu'
    pod 'SwiftLint'

    target 'aspaceTests' do
        inherit! :search_paths
        # Pods for testing
    end

    target 'aspaceUITests' do
        inherit! :search_paths
        # Pods for testing
    end
end
